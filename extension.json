{
	"name": "SearchExtraNS",
	"author": [
		"Roland Unger",
		"Hans Musil"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SearchExtraNS",
	"descriptionmsg": "searchextrans-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"SearchExtraNS": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SearchExtraNS\\": "includes/"
	},
	"Hooks": {
		"SearchAfterNoDirectMatch": [
			"MediaWiki\\Extension\\SearchExtraNS\\Hooks::NearMatch"
		]
	},
	"config": {
		"SearchExtraNamespaces": {
			"value": false
		}
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.31.0"
	}
}
